<div class="item group">
    <h3>User Profile</h3>
    <ul>
      <li>Email Address: {{{results.email}}}</li>
      <li>Social Media Network: {{{results.social_media_account}}}</li>
      <li>Birth Date: {{{results.birthdate}}}</li>
      <li>Gender: {{{results.gender}}}</li>
      <li>Interests:
      <ul class="interests">
          {{#each results.interests}}
          <li>{{this}}</li>
          {{/each}}
      </ul>
      </li>
      <li>Zip Code: {{{results.zip_code}}}</li>
      <li>Number of Followers: {{{results.followers}}}</li>
    </ul>
  </div>
